<?xml version="1.0" encoding="utf-8"?>
<com.chenantao.autolayout.AutoLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:Android="http://schemas.android.com/tools"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="70px"
        android:background="#4fa5fc"
        android:orientation="horizontal"
        android:gravity="center">

        <ImageView
            android:id="@+id/backs"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@mipmap/back"
            android:layout_centerVertical="true"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="请选择行业"
            android:textSize="28px"
            android:textColor="#ffffff"
            android:layout_weight="1"
            android:gravity="center"
            android:layout_centerInParent="true"
           />
        <ImageView
            android:id="@+id/my_check"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="24px"
            android:src="@mipmap/pulse_chose_add"/>

    </LinearLayout>


    <LinearLayout
        android:layout_width="672px"
        android:layout_height="60px"
        android:layout_marginTop="24px"
        android:background="@mipmap/searchbox"
        android:layout_gravity="center_horizontal"
        Android:focusable="true"
        android:focusableInTouchMode="true">

        <ImageView
            android:layout_width="42px"
            android:layout_height="38px"
            android:src="@mipmap/sear"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="12px"/>

        <EditText
            android:textCursorDrawable="@drawable/edit_cursor_color"
            android:layout_width="595px"
            android:layout_height="60px"
            android:layout_gravity="center_vertical"
            android:hint="搜索关键字"
            android:alpha="0.5"
            android:background="@null"
            android:layout_marginLeft="20px"
            android:textSize="24px"
            android:textColor="#979797"/>

    </LinearLayout>
    <ListView
        android:layout_marginTop="24px"
        android:id="@+id/item_view"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        ></ListView>

