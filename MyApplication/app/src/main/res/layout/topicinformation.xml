<?xml version="1.0" encoding="utf-8"?>
<com.chenantao.autolayout.AutoRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/detail_ra"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:Android="http://schemas.android.com/tools"
    app:layout_auto_basewidth="height"
    app:layout_auto_baseheight="width"
    android:fitsSystemWindows="true"
    android:orientation="vertical"
    android:background="#f6f6f6"
    android:clipToPadding="true">
    <TextView
        android:id="@+id/top"
        android:layout_width="match_parent"
        android:layout_height="25dp"
        android:background="@color/white"
        />
    <RelativeLayout
        android:id="@+id/heart"
        android:layout_below="@+id/top"
        android:layout_width="match_parent"
        android:layout_height="80px"
        android:background="@color/whrite"
        >
        <ImageView
            android:layout_centerVertical="true"
            android:id="@+id/back"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@mipmap/back"
            android:layout_gravity="center"/>
        <TextView
            android:id="@+id/deli_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="主体信息"
            android:layout_centerInParent="true"
            android:textColor="@color/title_head"
            android:textSize="@dimen/biaoti"/>
        <ImageView
            android:id="@+id/share"
            android:visibility="gone"
            android:layout_marginRight="19px"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:src="@mipmap/dimension_share"

            />
    </RelativeLayout>
    <ScrollView
        android:layout_below="@+id/heart"
        android:layout_above="@+id/bottomlay"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        >
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            >
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:id="@+id/title"
                android:background="@color/white"
                >
                <LinearLayout
                    android:id="@+id/left_click"
                    android:layout_marginTop="35dp"
                    android:layout_marginLeft="29dp"
                    android:layout_width="match_parent"
                    android:layout_height="35dp"
                    android:layout_weight="1.0"
                    android:gravity="center"
                    android:background="@drawable/left_draw_down"
                    >
                    <ImageView
                        android:id="@+id/left_img"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:scaleType="centerCrop"
                        android:src="@mipmap/qiye_h"
                        />
                    <TextView
                        android:id="@+id/left_subject"
                        android:layout_marginLeft="5dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="企业主体"
                        android:textColor="#ffffff"
                        android:textSize="14sp"
                        />

                </LinearLayout>
                <LinearLayout
                    android:id="@+id/right_click"
                    android:layout_marginTop="35dp"
                    android:layout_marginRight="29dp"
                    android:layout_width="match_parent"
                    android:layout_height="35dp"
                    android:gravity="center"
                    android:layout_weight="1.0"
                    android:background="@drawable/right_draw_up"
                    >

                    <ImageView
                        android:id="@+id/right_img"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:scaleType="centerCrop"
                        android:src="@mipmap/geren"
                        />
                    <TextView
                        android:id="@+id/right_subject"
                        android:layout_marginLeft="5dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="个人主体"
                        android:textColor="#5b9dff"
                        android:textSize="14sp"
                        />

                </LinearLayout>
            </LinearLayout>
            <LinearLayout
