                        android:visibility="gone"
                        android:gravity="center_vertical"
                        android:id="@+id/yuan_lay"
                        >
                        <TextView
                            android:id="@+id/yuan"
                            android:layout_centerVertical="true"
                            android:textSize="12sp"
                            android:textColor="#ffffff"
                            android:layout_width="19dp"
                            android:layout_height="19dp"
                            android:gravity="center"
                            android:visibility="visible"
                            android:background="@drawable/shape_yuan"
                             />
                         <ImageView
                             android:layout_marginLeft="5dp"
                             android:layout_width="wrap_content"
                             android:layout_height="wrap_content"
                             android:background="@mipmap/right_three"
                             />
                    </LinearLayout>

                    <TextView
                        android:layout_alignParentBottom="true"
                        android:layout_below="@+id/wukeyong"
                        android:id="@+id/youhuijuan_count"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:textColor="#ff5a4d"
                        android:textSize="15sp"
                        android:visibility="gone"
                        android:textStyle="bold"
                        android:text="-￥500"
                        android:layout_marginRight="24dp"
                        android:layout_alignParentRight="true"
                        android:drawableRight="@mipmap/right_three"
                        android:drawablePadding="5dp"
                        android:layout_marginBottom="10dp"
                        />

                </RelativeLayout>
                <TextView
                    android:layout_width="fill_parent"
                    android:layout_height="0.5px"
                    android:background="@color/lines"/>
            </LinearLayout>
            <RelativeLayout
                android:layout_below="@+id/feiyong"
                android:layout_marginTop="15dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                >
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="#3385ff"
                    android:text="￥0"
                    android:id="@+id/jiage"
                    android:layout_marginRight="24dp"
                    android:textSize="18sp"
                    android:layout_alignParentRight="true"
                    />
                <TextView
                    android:layout_marginTop="3dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_toLeftOf="@+id/jiage"
                    android:text="小计："
                    android:textColor="#696969"
                    android:textSize="13sp"
                    />
                <TextView
                    android:layout_below="@+id/jiage"
                    android:layout_marginTop="5dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="24dp"
                    android:text="*本报价不包含官费"
                    android:textColor="#c3c3c3"
                    android:textSize="9sp"
                    />

            </RelativeLayout>
        </RelativeLayout>
    </ScrollView>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:id="@+id/bottomlay"
        android:orientation="vertical"
        >
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:background="#eaeaea"
            >
            <TextView
                android:id="@+id/pay_money"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="待支付￥0"
                android:layout_alignParentLeft="true"
                android:layout_centerVertical="true"
                android:layout_marginLeft="14dp"
                android:textSize="18sp"
                android:textColor="#181818"
                />
            <TextView
                android:id="@+id/fenge"
                android:layout_toRightOf="@+id/pay_money"
                android:layout_width="1dp"
                android:layout_height="35dp"
                android:layout_marginLeft="5dp"
                android:background="@color/lines"
                android:layout_centerVertical="true"
                />
            <TextView
                android:id="@+id/youhui"
                android:layout_toRightOf="@+id/fenge"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="优惠￥0"
                android:visibility="visible"
                android:layout_centerVertical="true"
                android:layout_marginLeft="5dp"
                android:textSize="12sp"
                android:textColor="@color/text_gray"
                />
            <TextView
                android:id="@+id/pay_confirm"
                android:layout_toRightOf="@+id/youhui"
                android:layout_marginLeft="30dp"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="支付"
                android:gravity="center"
                android:textSize="18sp"
                android:textColor="@color/white"
                android:layout_alignParentRight="true"
                android:background="@color/lansecolor"
                />

        </RelativeLayout>
        <TextView
            android:id="@+id/bottmon_title"
            android:layout_width="match_parent"
            android:visibility="gone"
            android:background="@color/white"
            android:layout_height="25dp" />
    </LinearLayout>


</com.chenantao.autolayout.AutoRelativeLayout>