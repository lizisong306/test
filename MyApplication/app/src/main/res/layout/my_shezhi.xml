<?xml version="1.0" encoding="utf-8"?>
<com.chenantao.autolayout.AutoLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    app:layout_auto_basewidth="height"
    app:layout_auto_baseheight="width"
    android:fitsSystemWindows="true"
    android:gravity="center_horizontal"
    android:clipToPadding="true"
    android:background="#f6f6f6"
  >
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="80px"
        android:background="@color/whrite"
        >
        <ImageView
            android:layout_centerVertical="true"
            android:id="@+id/shezhi_backs"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@mipmap/back"

            android:layout_gravity="center"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="设置"
            android:layout_centerInParent="true"
            android:textColor="@color/title_head"
            android:textSize="@dimen/biaoti"/>
    </RelativeLayout>
    <TextView
        android:layout_width="fill_parent"
        android:layout_height="0.5px"
        android:background="@color/lines"/>
    <LinearLayout
        android:background="@color/whrite"
        android:layout_width="match_parent"
        android:layout_height="86px"
        android:orientation="horizontal"
        android:id="@+id/push_state"
        android:gravity="center">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="推送通知"
            android:layout_weight="1"
            android:layout_marginLeft="24px"
            android:textSize="26px"
            android:textColor="#444444"/>
        <view.SwitchButton
            android:id="@+id/my_kaiguan"

            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"

            />
    </LinearLayout>
    <TextView
        android:layout_width="fill_parent"
        android:layout_height="0.5px"
        android:background="@color/lines"/>

    <LinearLayout
        android:background="@color/whrite"
        android:layout_width="match_parent"
        android:layout_height="86px"
        android:visibility="gone"
        android:orientation="horizontal"
        android:gravity="center">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="收藏时转发"
            android:layout_weight="1"
            android:layout_marginLeft="24px"
            android:textSize="26px"
            android:textColor="#444444"/>
        <view.SwitchButton
            android:id="@+id/my_collects"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            />
    </LinearLayout>
    <TextView
        android:visibility="gone"
        android:layout_width="fill_parent"
        android:layout_height="0.5px"
        android:background="@color/lines"/>

    <LinearLayout
        android:background="@color/whrite"
        android:layout_width="match_parent"
        android:layout_height="86px"
        android:orientation="horizontal"
        android:gravity="center">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="非Wifi下不加载图片"
            android:layout_weight="1"
            android:layout_marginLeft="24px"
            android:textSize="26px"
            android:textColor="#444444"/>
        <view.SwitchButton
            android:id="@+id/pic_whife"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            />
    </LinearLayout>
    <TextView
        android:layout_width="fill_parent"
        android:layout_height="0.5px"
        android:background="@color/lines"/>


    <LinearLayout
        android:background="@color/whrite"
        android:id="@+id/clean_cache"
        android:layout_width="match_parent"
        android:layout_height="86px"
        android:orientation="horizontal"
        android:gravity="center">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="清理缓存"
            android:layout_weight="1"
            android:layout_marginLeft="24px"
            android:textSize="26px"
            android:textColor="#444444"/>
        <TextView
            android:id="@+id/clean_txt"
            android:layout_weight="1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="right"
            android:layout_marginRight="24px"
            android:textSize="26px"
            android:textColor="#aaaaaa"
            android:text="999M"/>
    </LinearLayout>
    <TextView
        android:layout_width="fill_parent"
        android:layout_height="0.5px"
        android:background="@color/lines"/>

    <LinearLayout
        android:id="@+id/xiugai_mima"
        android:background="@color/whrite"
        android:layout_width="match_parent"
        android:layout_height="86px"
        android:orientation="horizontal"
        android:gravity="center">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="修改密码"
            android:layout_weight="1"
            android:layout_marginLeft="24px"
            android:textSize="26px"
            android:textColor="#444444"/>
        <ImageView
            android:layout_width="12px"
            android:layout_height="22px"
            android:gravity="right"
            android:layout_marginRight="24px"
            android:background="@mipmap/safari_back"
            />
    </LinearLayout>
    <TextView
        android:id="@+id/xiugaimima_line"
        android:layout_width="fill_parent"
        android:layout_height="0.5px"
        android:background="@color/lines"/>

    <LinearLayout
        android:id="@+id/upgrade"
        android:background="@color/whrite"
        android:layout_width="match_parent"
        android:layout_height="86px"
        android:orientation="horizontal"
        android:gravity="center">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="版本升级"
            android:layout_weight="1"
            android:layout_marginLeft="24px"
            android:textSize="26px"
            android:textColor="#444444"/>
        <ImageView
            android:layout_width="12px"
            android:layout_height="22px"
            android:gravity="right"
            android:layout_marginRight="24px"
            android:background="@mipmap/safari_back"
            />
    </LinearLayout>
    <TextView
        android:layout_width="fill_parent"
        android:layout_height="0.5px"
        android:background="@color/lines"/>

    <LinearLayout
        android:visibility="gone"
        android:background="@color/whrite"
        android:layout_width="match_parent"
        android:layout_height="86px"
        android:id="@+id/help_"
        android:orientation="horizontal"
        android:gravity="center">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="使用帮助"
            android:layout_weight="1"
            android:layout_marginLeft="24px"
            android:textSize="26px"
            android:textColor="#444444"/>
        <ImageView
            android:layout_width="12px"
            android:layout_height="22px"
            android:gravity="right"
            android:layout_marginRight="24px"
            android:background="@mipmap/safari_back"
            />
    </LinearLayout>


    <LinearLayout
        android:background="@color/whrite"
        android:id="@+id/msg_feedback"
        android:layout_width="match_parent"
        android:layout_height="86px"
        android:orientation="horizontal"
        android:gravity="center">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="意见反馈"
            android:layout_weight="1"
            android:layout_marginLeft="24px"
            android:textSize="26px"
            android:textColor="#444444"
            android:id="@+id/textView3" />

        <ImageView

            android:layout_width="12px"
            android:layout_height="22px"
            android:gravity="right"
            android:layout_marginRight="24px"

            android:background="@mipmap/safari_back"
            />
    </LinearLayout>
    <TextView
        android:layout_width="fill_parent"
        android:layout_height="0.5px"
        android:background="@color/lines"/>
    <LinearLayout
        android:background="@color/whrite"
        android:id="@+id/erweima"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="86px"
        android:orientation="horizontal"
        android:gravity="center">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="推荐二维码"
            android:layout_weight="1"
            android:layout_marginLeft="24px"
            android:textSize="26px"
            android:textColor="#444444"/>
        <ImageView

            android:layout_width="12px"
            android:layout_height="22px"
            android:gravity="right"
            android:layout_marginRight="24px"

            android:background="@mipmap/safari_back"
            />
    </LinearLayout>
    <TextView
        android:visibility="gone"
        android:layout_width="fill_parent"
        android:layout_height="0.5px"
        android:background="@color/lines"/>
    <LinearLayout
        android:background="@color/whrite"
        android:id="@+id/about"
        android:layout_width="match_parent"
        android:layout_height="86px"
        android:orientation="horizontal"
        android:gravity="center">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="关于"
            android:layout_weight="1"
            android:layout_marginLeft="24px"
            android:textSize="26px"
            android:textColor="#444444"/>
        <ImageView

            android:layout_width="12px"
            android:layout_height="22px"
            android:gravity="right"
            android:layout_marginRight="24px"

            android:background="@mipmap/safari_back"
            />
    </LinearLayout>
    <TextView
        android:layout_width="fill_parent"
        android:layout_height="0.5px"
        android:background="@color/lines"/>

    <Button
        android:id="@+id/exit"
        android:layout_marginTop="34px"
        android:layout_width="672px"
        android:layout_height="70px"
        android:background="@mipmap/zxlogin"
        android:text="退出"
        android:textColor="@color/white"/>
</com.chenantao.autolayout.AutoLinearLayout>
